<!DOCTYPE html>
<html>
<head>
  <title>Application Layout</title>
  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
</head>
<body>
  <div class="container">

    <nav class="navbar navbar-expand-lg navbar-light bg-light">
      <a class="navbar-brand" href="#">Navbar</a>

      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="nav navbar-nav">
          <li class="nav-item">
            <a class="nav-link" href="#">Home</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">About</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">Contact</a>
          </li>
        </ul>
      </div>
    </nav>

    <div class="row">
      <div class="col-lg-4">
        <p>
          row 1 col 1
          <button type="button" class="btn btn-primary">Light</button>
        </p>
      </div>
      <div class="col-lg-4">
        <p>
          row 1 col 2
          <button type="button" class="btn btn-primary">Light</button>
        </p>
      </div>
      <div class="col-lg-4">
        <p>
          row 1 col 3
          <button type="button" class="btn btn-primary">Light</button>
        </p>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-4">
        <p>
          row 2 col 1
          <button type="button" class="btn btn-primary">Light</button>
        </p>
      </div>
      <div class="col-lg-4">
        <p>
          row 2 col 2
          <button type="button" class="btn btn-primary">Light</button>
        </p>
      </div>
      <div class="col-lg-4">
        <p>
          row 2 col 3
          <button type="button" class="btn btn-primary">Light</button>
        </p>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-4">
        <p>
          row 3 col 1
          <button type="button" class="btn btn-primary">Light</button>
        </p>
      </div>
      <div class="col-lg-4">
        <p>
          row 3 col 2
          <button type="button" class="btn btn-primary">Light</button>
        </p>
      </div>
      <div class="col-lg-4">
        <p>
          row 3 col 3
          <button type="button" class="btn btn-primary">Light</button>
        </p>
      </div>
    </div>
  </div>
  <%= yield %>
  <%= javascript_include_tag "application" %>
</body>
</html>
